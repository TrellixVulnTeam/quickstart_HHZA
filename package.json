{
  "name": "quickstart",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prod": "set NODE_ENV=production & node ./bin/www",
    "debug": "set DEBUG=quickstart:* & nodemon ./bin/www",
    "dev": "nodemon ./bin/www",
    "prettier": "prettier --write --tab-width 4 --single-quote **/*.{ts,js,css,html}",
    "test": "mocha ./test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "debug": "^2.6.9",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "finale-rest": "^1.1.1",
    "http-errors": "^1.7.3",
    "morgan": "^1.10.0",
    "node-pre-gyp": "^0.14.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "sequelize": "^5.21.13"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "repository": "https://github.com/WeiLiangLOL/quickstart",
